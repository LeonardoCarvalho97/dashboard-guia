{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/(.*)",
      "dest": "/",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Content-Type": "application/json"
      }
    },
    {
      "src": "/(.*)/",
      "dest": "/",
      "methods": ["PUT", "DELETE"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Content-Type": "application/json"
      }
    }
  ]
}
