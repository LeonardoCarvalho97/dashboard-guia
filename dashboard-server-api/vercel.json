{
    "version": 2,
    "builds": [
      {
          "src": "./index.js",
          "use": "@vercel/node"
      }
    ],
    "routes": [
      { 
        "handle": "filesystem",
        "src": "/employees",
        "methods": ["GET"],
        "headers": {
          "Access-Control-Allow-Origin": "*"
        } 
      },
      {
        "src": "/(.*)",
        "dest": "/",
        "methods": ["GET", "POST", "PUT", "DELETE"]
      }
    ]
  }
  